        <Converters:GridEnumToBoolConverter x:Key="GridEnumToBoolConverter"/> 
        <Converters:SelectionAnsConverterOne x:Key="SelectionAnsConverterOne"/>
        <RadioButtonConverter:RadioButtonIntConverter x:Key="RadioMultiIntConverter">
            </RadioButtonConverter:RadioButtonIntConverter>
    </UserControl.Resources>
    
    <Grid HorizontalAlignment="Left">
