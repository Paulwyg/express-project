            <RowDefinition Height="Auto" MinHeight="30"/>
            <RowDefinition Height="*" MinHeight="30"/>
            <RowDefinition Height="Auto" MinHeight="30"/>
            <!--<RowDefinition Height="Auto" MinHeight="30"/>-->

        </Grid.RowDefinitions>
        <TextBlock Visibility="Collapsed"  Width="{Binding Hightxx}" Height="0" x:Name="txb"/>

        <!--<Views:TmlFaultsShieldView Margin="0,0,0,5" Visibility="{Binding ShowFault, Converter={StaticResource BoolToVisible}}" Grid.Row="1"/>-->
        <Grid Grid.Row="1" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="25,5,0,0"> 
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto"></ColumnDefinition>
                <ColumnDefinition Width="Auto"></ColumnDefinition>
                <ColumnDefinition Width="Auto"></ColumnDefinition>
                <ColumnDefinition Width="Auto"></ColumnDefinition>
                <ColumnDefinition Width="*"></ColumnDefinition>
                <ColumnDefinition Width="Auto"></ColumnDefinition>
                <ColumnDefinition Width="Auto"></ColumnDefinition>
                <ColumnDefinition Width="Auto"></ColumnDefinition>
                <ColumnDefinition Width="*"></ColumnDefinition>
                <ColumnDefinition Width="Auto"></ColumnDefinition>
                <ColumnDefinition Width="25"></ColumnDefinition>
                <ColumnDefinition Width="Auto"></ColumnDefinition>
                <ColumnDefinition Width="25"></ColumnDefinition>
                <ColumnDefinition Width="Auto"></ColumnDefinition>
            </Grid.ColumnDefinitions>
            <RadioButton Content="全局分组" Margin="0,10,0,5" IsEnabled="{Binding RdGbIsEnable}" IsChecked="{Binding RdIndex, Converter={StaticResource RadioMultiIntConverter}, ConverterParameter=1}" GroupName="x1"></RadioButton>
            <RadioButton Content="本地分组"  Margin="15,10,0,5" Grid.Column="1" IsEnabled="{Binding RdGbIsEnable}" IsChecked="{Binding RdIndex, Converter={StaticResource RadioMultiIntConverter}, ConverterParameter=2}"  GroupName="x1"></RadioButton>
