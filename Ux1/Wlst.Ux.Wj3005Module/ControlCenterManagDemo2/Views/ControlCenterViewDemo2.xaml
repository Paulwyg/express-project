            <TextBlock Text="{Binding ColNoAlarmTimeMinute}" HorizontalAlignment="Right" Margin="1,5,0,5" Grid.Column="12" Visibility="{Binding IsShowAlarmTime}" Foreground="Red"/>
            <TextBlock Text="秒" HorizontalAlignment="Right" Margin="1,5,20,5" Grid.Column="13" Visibility="{Binding IsShowAlarmTime}" Foreground="Red"/>
        </Grid>
        
