                                        <SplineDoubleKeyFrame KeyTime="00:00:00.6000000" Value="{Binding CanWidth}"/>
                                    </DoubleAnimationUsingKeyFrames>

                                    <DoubleAnimationUsingKeyFrames BeginTime="00:00:00.2000000" Duration="00:00:00.8000000" 
