                    </telerik:RadGridView>
                    <Label Grid.Row="3" HorizontalAlignment="Left" Margin="10,0,0,0" Content="{Binding Remind}"/>
                    <Button Grid.Row="3" Margin="0,0,70,0" HorizontalAlignment="Right"  Content="导 出" Width="120" Height="25" Click="Button_Click_3"></Button>

                </Grid>
            </TabItem>
        </TabControl>

    </Grid>
</UserControl>

