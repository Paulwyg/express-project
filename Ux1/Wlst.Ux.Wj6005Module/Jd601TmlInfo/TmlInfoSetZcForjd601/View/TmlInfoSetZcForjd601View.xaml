                        <TextBlock  Foreground="{Binding ElementName=fg, Path=Foreground}"  Margin="5,2,5,2" Grid.Row="2" Grid.Column="1" Name="textBlock111" Text="{Binding ZcJd601ParItems[2].Name}" VerticalAlignment="Center" MinWidth="60" />

