                </Trigger>
                <Trigger Property="IsMouseOver" Value="True">
                    <Setter TargetName="Background_Over" Property="Visibility" Value="Visible" />
                    <Setter TargetName="Background_Over" Property="BorderBrush" Value="{StaticResource ItemOuterBorder_SelectedUnfocused}" />
                <Setter TargetName="BackgroundInner_Over" Property="Background" Value="{Binding RelativeSource={RelativeSource Self}, Path=(RadGridViewSet:RadGridViewAttriXaml.ItemBackgroundMouseOver), Mode=OneWay}" />
                </Trigger>
