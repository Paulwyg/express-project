                                </Path>
                                <TextBlock Margin="0,0,0,0" MinWidth="90" Visibility="{Binding FaultCategories[3].IsVisi}">
                                      <Run Text="其他:"  />
                                      <Run x:Name="others" FontWeight="Heavy"/>
                                </TextBlock>
                                <Path  Fill="Red" Margin="10,0,5,0" Visibility="{Binding FaultCategories[4].IsVisi}">
                                    <Path.Data>
                                        <RectangleGeometry Rect="0,3,10,10"/>
                                    </Path.Data>
                                </Path>
                                <TextBlock Margin="0,0,0,0" MinWidth="90" Visibility="{Binding FaultCategories[4].IsVisi}">
                                      <Run Text="总数:"  />
                                      <Run x:Name="sumnumber" FontWeight="Heavy"/>
                                </TextBlock>
                                <Path  Fill="Blue" Margin="10,0,5,0" Visibility="{Binding VisiOne}">
                                    <Path.Data>
                                        <RectangleGeometry Rect="0,3,10,10"/>
                                    </Path.Data>
                                </Path>
                                <TextBlock Margin="0,0,0,0" MinWidth="90" Visibility="{Binding VisiOne}">
                                      <Run Text="单个:"  />
                                      <Run x:Name="onefault" FontWeight="Heavy"/>
                                </TextBlock>
                            </StackPanel>
                            <!--<StackPanel  Grid.Column="1" Orientation="Horizontal" HorizontalAlignment="Right" Margin="0,5,20,5">
                                <Path  Fill="Blue" Margin="5,0">
                                    <Path.Data>
                                        <RectangleGeometry Rect="0,3,10,10"/>
                                    </Path.Data>
                                </Path>
                                <TextBlock Text="单个故障" />
                            </StackPanel>-->
