                                        <RectangleGeometry Rect="0,3,10,10"/>
                                    </Path.Data>
                                </Path>
                                <TextBlock MinWidth="90" Visibility="{Binding FaultCategories[0].IsVisi}">
                                     <Run Text="终端:"  />
                                     <Run x:Name="terminal" FontWeight="Heavy"/>
                                </TextBlock>
                                <Path  Fill="Green" Margin="10,0,5,0" Visibility="{Binding FaultCategories[1].IsVisi}">
                                    <Path.Data>
                                        <RectangleGeometry Rect="0,3,10,10"/>
