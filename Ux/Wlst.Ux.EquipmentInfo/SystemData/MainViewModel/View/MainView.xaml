                    </Grid>-->
                    <GridSplitter Grid.Row="1" Grid.ColumnSpan="2"  HorizontalAlignment="Stretch"  VerticalAlignment="Center" Height="3"></GridSplitter>
                    <Grid  Grid.Row="2" x:Name="RadChart3">
