                            </StackPanel>
                            <TextBlock Grid.Column="1"  Text="系统故障：        故障名称之前的序号与故障分布图中的序号相对应" Margin="10,10,0,5"  HorizontalAlignment="Left" VerticalAlignment="Top"/>
                            <ListBox  x:Name="one" Grid.Row="1" Grid.Column="1" ItemsSource="{Binding TypeItems }" Margin="10,5,10,0">
